etDarkMode] = useState(storedDarkMode);\n\n    const setMode = mode => {\n        localStorage.setItem('darkMode', mode);\n        setDarkMode(mode);\n    }\n\n    const toggleDarkMode = () => {\n        setMode(darkMode === 'dark' ? 'light' : 'dark');\n    };\n\n    useEffect(() => {\n        if (darkMode === 'dark') {\n            body.classList.add('dark-mode');\n        } else {\n            body.classList.remove('dark-mode');\n        }\n        setComponentMounted(true);\n    }, [darkMode]);\n\n    return [darkMode, toggleDarkMode, componentMounted]\n}","/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/context/auth.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/components/PrivateRoute.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/i18next.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/pages/MyPlan.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/pages/Impressum.js",["94"],"import React from \"react\";\nimport Header from \"../components/header/Header\";\nimport Footer from \"../components/footer/Footer\";\nimport {useTranslation} from \"react-i18next\";\n\nfunction Impressum() {\n\n    const {t, i18n} = useTranslation();\n\n    return (\n        <div>\n            <Header/>\n            <div className=\"col-12 text-center mt-5\">\n                <h2>Impressum page</h2>\n                <p>{t(\"datenschutz\")}</p>\n            </div>\n            <Footer/>\n        </div>\n    )\n}\n\nexport default Impressum;","/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/pages/Datenschutz.js",["95","96"],"import React from \"react\";\nimport Header from \"../components/header/Header\";\nimport Footer from \"../components/footer/Footer\";\nimport {useTranslation} from \"react-i18next\";\n\nfunction Datenschutz() {\n\n    const {t, i18n} = useTranslation();\n\n    return (\n        <div>\n            <Header/>\n            <div className=\"col-12 text-center mt-5\">\n                <h2>D