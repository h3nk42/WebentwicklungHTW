s.env.REACT_APP_API_URL;\n    const history = useHistory();\n\n    const checkEmptyField = () => {\n        return userName === '' || password === '' || confirmPassword === '';\n    }\n\n    function handleUsernameChange(e) {\n        setUserName(e.target.value);\n    }\n\n    function handlePasswordChange(e) {\n        setPassword(e.target.value);\n    }\n\n    function handleConfirmPasswordChange(e) {\n        setConfirmPassword(e.target.value);\n    }\n\n    async function createUser(e) {\n        e.preventDefault();\n        if (password !== confirmPassword) {\n            setRegisterState(\"match_error\");\n        } else {\n            setRegisterState(\"registering\");\n            let data = {\n                password: password,\n                userName: userName\n            }\n            try {\n                let res = await axios({\n                    method: 'post',\n                    url: `${API_URL}user/create`,\n                    data: data\n                });\n                if (res.status === 200) {\n                    setRegisterState(\"registered\");\n                    history.push(\"/login\");\n                }\n            } catch (e) {\n                setRegisterState(\"error\");\n            }\n        }\n    }\n\n    return (\n        <div data-testid=\"register-card\" className=\"card p-4 col-12 col-lg-4 login-card card-style\">\n            {registerState === \"registering\" ?\n                <div className=\"spin\"></div>\n                :\n                <>\n                    <h2 className=\"card-title mt-4\">Register</h2>\n                    <p className=\"card-subtitle mb-5\">Already have an account?\n                        <span data-testid=\"sign-in\"\n                                className=\"btn btn-outline-primary\"\n                                onClick={() => history.push(\"/login\")}\n                        >\n                            Sign in here\n                        </span>\n                    </p>\n                    <form data-testid=\"registration-form\">\n                        <div className=\"form-group text-left\">\n                            <label>Username :</label>\n                            <input data-testid=\"username-register\"\n                                   type=\"text\"\n                                   className=\"form-control\"\n                                   placeholder=\"Username\"\n                                   value={userName}\n                                   onChange={handleUsernameChange}\n                            />\n                        </div>\n                        <div className=\"form-group text-left\">\n                            <label>Password :</label>\n                            <input data-testid=\"password-register\"\n                                   type=\"password\"\n                                   className=\"form-control\"\n                                   placeholder=\"Password\"\n                                   value={password}\n                                   onChange={handlePasswordChange}\n                            />\n                        </div>\n                        <div className=\"form-group mb-5 text-left\">\n                            <label>Confirm Password :</label>\n                            <input data-testid=\"confirm-password\"\n                                   type=\"password\"\n                                   className=\"form-control\"\n                                   placeholder=\"Confirm Password\"\n                                   value={confirmPassword}\n                                   onChange={handleConfirmPasswordChange}\n                            />\n                        </div>\n                        <div className=\"text-center\">\n                            <button\n                                data-testid=\"register-btn\"\n                                type=\"submit\"\n                                className=\"btn btn-primary btn-block mb-4\"\n                                onClick={createUser}\n                                disabled={checkEmptyField()}\n                            >\n                                Register\n                            </button>\n                        </div>\n                    </form>\n                    {registerState === \"match_error\" && \"Passwords do not match!\"}\n                    {registerState === \"error\" && \"Username is invalid!\"}\n                </>\n            }\n        </div>\n    );\n}\n\nexport default RegistrationForm;\n","/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/pages/Home.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/hooks/useDarkMode.js",["100"],"import {useState, useEffect} from 'react';\n\nexport default function UseDarkMode() {\n    const body = document.querySelector(\"body\");\n    const storedDarkMode = localStorage.getItem(\"darkMode\");\n\n    const [componentMounted, setComponentMounted] = useState(false);\n    const [darkMode, setDarkMode] = useState(storedDarkMode);\n\n    const setMode = mode => {\n        localStorage.setItem('darkMode', mode);\n        setDarkMode(mode);\n    }\n\n    const toggleDarkMode = () => {\n        setMode(darkMode === 'dark' ? 'light' : 'dark');\n    };\n\n    useEffect(() => {\n        if (darkMode === 'dark') {\n            body.classList.add('dark-mode');\n        } else {\n            body.classList.remove('dark-mode');\n        }\n        setComponentMounted(true);\n    }, [darkMode]);\n\n    return [darkMode, toggleDarkMode, componentMounted]\n}","/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/context/auth.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/components/PrivateRoute.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/i18next.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/client/src/pages/MyPlan.js",[],"/Users/Sunan/Projects/react/WebentwicklungHTW/clie